    "import pandas as pd\n",
    "import os\n",
    "ruta = os.getcwd()\n",
    "pathx=(f\"{ruta}/datos.xlsx\")\n",
    "path=(f\"{ruta}/datos.json\")\n",
    "\n",
    "dfx = pd.read_excel(pathx)\n",
    "# nota instalar pip install openpyxl\n",
    "df = pd.read_json(path)\n",
    "\n",
    "\n",
    "result = pd.concat([df,dfx], ignore_index=True,axis=1)\n",
    "result\n",
    "\n"
